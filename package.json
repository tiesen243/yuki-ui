{
  "name": "yuki-ui",
  "private": true,
  "homepage": "https://yuki-ui.vercel.app",
  "bugs": {
    "url": "https://github.com/tiesen243/yuki-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiesen243/yuki-ui.git"
  },
  "workspaces": [
    "kaze",
    "examples/*",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && git clean -xdf node_modules",
    "dev": "turbo dev --continue",
    "format": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "turbo": "^2.8.12"
  },
  "packageManager": "bun@1.3.0",
  "catalogs": {
    "prod": {
      "@tanstack/react-query": "^5.90.12",
      "zod": "4.3.6"
    },
    "dev": {
      "@types/node": "^25.3.3",
      "dotenv-cli": "^11.0.0",
      "tsdown": "^0.19.0-beta.5",
      "typescript": "^5.9.3"
    },
    "react": {
      "@types/react": "^19.2.14",
      "@types/react-dom": "^19.2.3",
      "react": "^19.2.4",
      "react-dom": "^19.2.4"
    },
    "tailwind": {
      "@tailwindcss/postcss": "^4.2.1",
      "@tailwindcss/vite": "^4.1.18",
      "tailwindcss": "^4.2.1"
    },
    "trpc": {
      "@trpc/server": "^11.8.1",
      "@trpc/client": "^11.8.1",
      "@trpc/tanstack-react-query": "^11.8.1",
      "superjson": "^2.2.6"
    },
    "orpc": {
      "@orpc/server": "^1.13.1",
      "@orpc/client": "^1.13.1",
      "@orpc/react-query": "^1.13.1"
    }
  }
}
